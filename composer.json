{
  "name": "phpro/mage2-module-bypass-page-cache",
  "description": "Bypass the page cache by adding a specific header to the request",
  "type": "magento2-module",
  "license": "MIT",
  "authors": [
    {
      "name": "PHPro NV",
      "email": "info@phpro.be",
      "homepage": "https://www.phpro.be/"
    }
  ],
  "require": {
    "php": "^7.0",
    "magento/framework": "^100.1.10|^101.0.1|^102.0|^103.0",
    "magento/module-page-cache": "^100.1.5"
  },
  "require-dev": {
    "magento/marketplace-eqp": "^1.0",
    "phpro/grumphp": "^0.13"
  },
  "autoload": {
    "psr-4": {
      "Phpro\\BypassPageCache\\": ""
    },
    "files": [
      "registration.php"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    },
    {
      "type": "git",
      "url": "https://github.com/magento/marketplace-eqp"
    }
  ],
  "config": {
    "platform": {
      "ext-gd": "7.3",
      "ext-mcrypt": "7.3",
      "ext-itl": "7.3",
      "ext-xsl": "7.3",
      "ext-bcmath": "7.3",
      "ext-pdo_mysql": "7.3",
      "ext-soap": "7.3",
      "ext-zip": "7.3"
    }
  }
}
